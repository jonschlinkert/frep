    "replacements",
    "search",
    "sluggifier",
    "slugification",
    "slugified",
    "slugify",
    "string",
    "strings",
    "structure",
    "substitute",
    "tranformation",
    "transform",
    "underscore",
    "url",
    "utility"
  ],
  "devDependencies": {
    "chai": "^1.9.1",
    "mocha": "^1.18.2",
    "underscore.string": "^2.3.3",
    "verb": "^0.2.7"
  },
  "dependencies": {
    "replacements": "^0.1.1"
  }
}
